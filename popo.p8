pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
player = {}
player.x = 20
player.y = 20
player.sprite = 1
player.speed = 1

function handle_mvt()
	if player.speed < 3 then
		player.speed += 1
	end
	
	if btn(0) then
		player.x -= player.speed
	end
	if btn(1) then
		player.x += player.speed
	end
	if btn(2) then
		player.y -= player.speed
	end
	if btn(3) then
		player.y += player.speed
	end
end

function _draw()
	cls()
	map(0)
	spr(player.sprite, player.x, player.y)
	pset(10, 10, 11)
end

function _update60()
	handle_mvt()
end
